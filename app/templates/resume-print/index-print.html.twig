<div id="page-wrapper" class="d-flex flex-column justify-content-between min-vh-100">

    <main class="position-relative z-0">
        <div class="bg-light">
            <div class="container-fluid">
                <article class="mx-auto {{ articlePrefix }}wrapper bg-white shadow-lg">
                    {% include 'resume-print/header.html.twig' %}
                    {% include 'resume-print/summary.html.twig' %}
                    {% include 'resume-print/body.html.twig' %}
                    {% include 'resume-print/footer.html.twig' %}
                </article>
            </div>
        </div>
    </main>

</div>


