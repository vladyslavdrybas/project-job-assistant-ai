{% set articlePrefix='resume--' %}
<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>
        {% block title %}
            {% if meta is defined and meta.title is defined %}
                {{ meta.title }}
            {% elseif title is defined %}
                {{ title }}
            {% else %}
                Welcome
            {% endif %}
        {% endblock %}
    </title>

    <link rel="stylesheet" href="{{ fileGetServerPath(asset('css/bootstrap-icons.min.css', 'bootstrap')) }}">
    <link rel="stylesheet" href="{{ fileGetServerPath(asset('css/bootstrap.min.css', 'bootstrap')) }}">
    <link rel="stylesheet" href="{{ fileGetServerPath(asset('style.css?t=' ~ 'now'|date('Y-m-d-H-i-s'), 'css')) }}">

<style>
    .row {
        display: -webkit-box;
    }

    .text-primary,
    .text-title-head
    {
        color: rgba(83,83,211, 1) !important;
    }

    .text-title-head {
        padding-left: 0.55rem;
        border-left: 0.55rem solid rgba(83,83,211, 1);
    }

    .shadow-border-start {
        border:none;
        border-left: 0.1rem solid rgba(83,83,211, 1);
    }

    .shadow-border-end {
        border:none;
        border-right: 0.1rem solid rgba(83,83,211, 1);
    }

    .shadow-border-top {
        border:none;
        border-top: 0.1rem solid rgba(83,83,211, 1);
    }

    .shadow-border-bottom {
        border:none;
        border-bottom: 0.1rem solid rgba(83,83,211, 1);
    }
