<h3>Contact person</h3>

<div class="row mb-3">
    <div class="col-6">
        {{ form_row(form.firstName) }}
    </div>

    <div class="col-6">
        {{ form_row(form.lastName) }}
    </div>
</div>

<div class="row mb-3">
    <div class="col-6">
        {{ form_row(form.contacts.email) }}
    </div>

    <div class="col-6">
        {{ form_row(form.contacts.phone) }}
    </div>
</div>
